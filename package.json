{
  "name": "nhmate-admin-dashboard",
  "version": "0.4.0",
  "description": "Admin panel dashboard for NHMate.",
  "private": true,
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "scripts": {
    "tailwind:dev": "npx tailwindcss-cli@latest build -i src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=development postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "0.3.1",
    "array-move": "^4.0.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.2.3",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "date-and-time": "^2.4.2",
    "dotenv": "^16.3.1",
    "faker": "4.1.0",
    "firebase": "^9.21.0",
    "jquery": "^3.6.3",
    "moment": "^2.29.4",
    "moment-js": "^1.1.15",
    "multiselect-react-dropdown": "^2.0.25",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.7.4",
    "react-chartjs-2": "2.9.0",
    "react-csv": "^2.2.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-horizontal-timeline": "^1.5.3",
    "react-icons": "^4.12.0",
    "react-markdown": "^8.0.5",
    "react-paginate": "^8.1.4",
    "react-redux": "^8.1.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "react-sortable-hoc": "^2.0.0",
    "react-spinners": "^0.13.8",
    "react-step-progress": "^1.0.3",
    "react-step-progress-bar": "^1.0.3",
    "react-tabs": "^4.0.0",
    "react-toastify": "^9.1.3",
    "react-transition-group": "4.4.1",
    "react-vertical-timeline-component": "^3.6.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "remark-gfm": "^3.0.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^8.0.0",
    "postcss-loader": "^7.3.0",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "^3.3.2"
  },
  "keywords": [
    "nhmate",
    "admin",
    "travel",
    "highways"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
